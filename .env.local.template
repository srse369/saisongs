# Oracle Autonomous Database Connection
# Get these from your Oracle Cloud Console
VITE_ORACLE_USER=your_oracle_username
VITE_ORACLE_PASSWORD=your_oracle_password
VITE_ORACLE_CONNECT_STRING=your_connection_string
# Example connect string: (description=(retry_count=20)(retry_delay=3)(address=(protocol=tcps)(port=1522)(host=your-host.oraclecloud.com))(connect_data=(service_name=your_service_name))(security=(ssl_server_dn_match=yes)))

# Admin Password for Bulk Import Feature
# Set a secure password for accessing the hidden bulk import functionality
# This password is required to access the import feature via Ctrl+Shift+I (Cmd+Shift+I on Mac)
VITE_ADMIN_PASSWORD=your_secure_admin_password_here

# API Server URL
# URL of the backend API server (default: http://localhost:3001/api)
VITE_API_URL=http://localhost:3001/api
